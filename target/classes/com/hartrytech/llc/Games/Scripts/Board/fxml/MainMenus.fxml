<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="MenusBox" alignment="CENTER" prefHeight="415.0" prefWidth="487.0" spacing="50.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hartrytech.llc.Games.Scripts.Board.fxml.Controller">
   <children>
      <HBox id="topMenus" alignment="CENTER_RIGHT" layoutX="30.0" layoutY="99.0" prefHeight="17.0" prefWidth="313.0">
         <effect>
            <Reflection fraction="0.79" topOpacity="0.68" />
         </effect>
      </HBox>
      <HBox alignment="CENTER" prefHeight="17.0" prefWidth="313.0">
         <children>
            <Label alignment="TOP_CENTER" prefHeight="30.0" prefWidth="93.0" text="Menus" textAlignment="CENTER" textFill="#944d4d">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <effect>
            <Reflection fraction="0.79" topOpacity="0.68" />
         </effect>
      </HBox>
      <VBox id="ButtonBox" alignment="CENTER" prefHeight="128.0" prefWidth="350.0" spacing="15.0">
         <children>
            <Button id="GameButton"  alignment="CENTER" contentDisplay="CENTER" onAction="#games" prefHeight="24.0" prefWidth="147.0" text="Games" />
            <Button mnemonicParsing="false" onAction="#settings" prefHeight="24.0" prefWidth="207.0" text="Settings" />
            <Button mnemonicParsing="false" onAction="#credits" prefHeight="24.0" prefWidth="253.0" text="Credits" />
         </children>
      </VBox>
      <Pane id="bottomMenus" prefHeight="80.0" prefWidth="450.0" />
   </children>
   <padding>
      <Insets left="5.0" right="5.0" top="20.0" />
   </padding>
</VBox>
